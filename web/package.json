{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "format:check": "yarn prettier --list-different \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "yarn prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:css": "stylelint --allow-empty-input src/**/*.styles.{ts,js}",
    "lint:js": "eslint src --no-error-on-unmatched-pattern --ext .js,.jsx",
    "lint:ts": "eslint src --ext .ts,.tsx",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "start": "cross-env BROWSER='Google Chrome' && react-app-rewired start"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.1.8"
  }
}
