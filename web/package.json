{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "format:check": "yarn prettier --list-different \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "yarn prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:css": "stylelint --allow-empty-input src/**/*.styles.{ts,js}",
    "lint:js": "eslint src --no-error-on-unmatched-pattern --ext .js,.jsx",
    "lint:ts": "eslint src --ext .ts,.tsx",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "start": "cross-env BROWSER='Google Chrome' && react-app-rewired start"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "formik": "^2.2.9",
    "formik-material-ui": "^3.0.1",
    "notistack": "^1.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-focus-lock": "^2.5.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/css-mediaquery": "^0.1.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.8",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "css-mediaquery": "^0.1.2",
    "customize-cra": "^1.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-testing-library": "^4.9.0",
    "eslint-plugin-you-dont-need-momentjs": "^1.6.0",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "prettier": "^2.3.2",
    "react-app-rewired": "^2.1.8",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-at-rule-no-children": "^0.3.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-declaration-use-variable": "^1.7.3",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-selector-no-empty": "^1.0.8",
    "stylelint-use-nesting": "^3.0.0",
    "typescript": "^4.3.5",
    "web-vitals": "^2.1.0"
  },
  "homepage": "./do-mongodb-feeling-app"
}
